{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es2017",
      "dom",
      "es6"
    ],
    "types": ["node"],
    "declaration": true,
    "outDir": "./dist",
    "typeRoots": [
      "node_modules/@types",
      "./typings",
    ],
    "jsx": "react",
    "moduleResolution": "node"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}